@model List<AppRole>
@{
    Layout = "_Layout";
}
<a asp-action="Create" asp-controller="Role" class="d-inline-block my-2 btn btn-warning">Yeni Rol Oluştur</a>
<table class="table table-bordered table-hovered table-sm">
    <thead>
        <tr>
            <th>Username</th>
            <th>Created Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.CreatedTime</td>
            </tr>
        }
    </tbody>
</table>